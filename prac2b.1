import java.util.*;

public class Competitve {

	public static void main (String args[]) {
		int num;
		Scanner scan = new Scanner(System.in);
		
		num = scan.nextInt();
		int sum=0;
		int prod = 1;
		//int arr[] = new int[num];
		System.out.println("Choose 1 for sum 2 for product");
		int choice = scan.nextInt();
		if(choice==1) {
		for(int i = 1; i <=num; i++)
        {
            sum = sum+i;
        }
		
		System.out.println("Sum="+sum);
	
	}
	else if(choice ==2) {
		for(int i = 1; i <=num; i++)
        {
            prod = prod*i;
        }
		
		System.out.println("Product="+prod);
	}
	else {
		System.out.println("Wrong option selected");
	}
		//System.out.println("Sum="+reverse);
	}
}